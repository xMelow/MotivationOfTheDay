{
    "name": "Quote of the Day",
    "version": "1.0.0",
    "short_name": "Quotes",
    "display": "standalone",
    "background_color": "#ffffff",
    "theme_color": "#000000",
    "manifest_version": 3,
    "author": "Flor Stellamans",
    "action": {
        "default_popup": "index.html",
        "default_title": "Quote of the Day"
    },
    "background": {
        "service_worker": "/assets/js/background.js"
    },
    "permissions": [
        "storage", 
        "windows",
        "tabs",
        "activeTab"
    ],
    "icon": {
        "192": "/assets/images/quote-icon-logo-design-template-vector.jpg"
    }
  }
  